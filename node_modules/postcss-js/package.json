    "ava":                   "0.14.0"
  },
  "scripts": {
    "test": "ava && eslint *.js test/*.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "rules": {
      "quote-props": 0
    }
  }
